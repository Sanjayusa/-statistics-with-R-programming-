i)
# Load the airquality dataset
data(airquality)
# Compute the mean temperature
mean_temp <- mean(airquality$Temp, na.rm = TRUE)
print(mean_temp)

ii)
# Load the airquality dataset
data(airquality)
# Extract the first five rows
first_five_rows <- head(airquality, n = 5)
print(first_five_rows)

iii)
# Load the airquality dataset
data(airquality)
# Extract all columns except Temp and Wind
airquality_except_temp_wind <- airquality[, -c(4, 3)]
print(airquality_except_temp_wind)

iv)
# Load the airquality dataset
data(airquality)
# Find the coldest day
coldest_day <- which.min(airquality$Temp)
print(coldest_day)

v)
# Load the airquality dataset
data(airquality)
# Find the number of days with wind speed greater than 17 mph
num_windy_days <- sum(airquality$Wind > 17, na.rm = TRUE)
print(num_windy_days)




