# define a function to generate the nth Fibonacci number
fibonacci <- function(n) {
  if (n == 0) {
    return(0)
  } else if (n == 1) {
    return(1)
  } else {
    return(fibonacci(n-1) + fibonacci(n-2))}}
# generate the first 10 Fibonacci numbers using the fibonacci function
for (i in 0:9) {
  result <- fibonacci(i)
  print(result)
}
