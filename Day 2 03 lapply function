# create a list
my_list <- list(c(1,2,3), c(4,5,6), c(7,8,9))
# define a function to calculate the mean of a vector
get_mean <- function(x) {
  mean(x)
}
# apply the get_mean function to each element of the list using lapply
mean_list <- lapply(my_list, get_mean)
# print the results
print(mean_list)
