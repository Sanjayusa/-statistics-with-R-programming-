# create the data frame XY
X <- c(1,2,3,1,4,5,2)
Y <- c(0,3,2,0,5,9,3)
XY <- data.frame(X, Y)

# view the original data frame
XY

# get the index of unique rows
unique_index <- !duplicated(XY)

# subset XY to keep only the unique rows
XY_unique <- XY[unique_index, ]

# view the resulting data frame
XY_unique
